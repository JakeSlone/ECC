{
  "version": 2,
  "buildCommand": "pnpm build",
  "outputDirectory": "dist/spa",
  "framework": "vite",
  "functions": {
    "api/index.ts": {
      "runtime": "nodejs22.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
